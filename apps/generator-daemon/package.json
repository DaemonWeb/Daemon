{
  "name": "generator-daemon",
  "version": "1.0.1",
  "description": "Scaffold applications based on starter projects from Daemon Web Tools",
  "homepage": "https://daemonweb.tools",
  "author": {
    "name": "Josh Elias",
    "email": "josh@daemonweb.tools",
    "url": "https://daemonweb.tools"
  },
  "main": "generators/index.js",
  "types": "generators/index.d.ts",
  "keywords": [
    "supabase",
    "strapi",
    "next",
    "yeoman-generator"
  ],
  "devDependencies": {
    "@types/node": "^18.14.2",
    "@types/yeoman-generator": "^5.2.11",
    "@types/yosay": "^2.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^3.1.0",
    "jest": "^26.6.3",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.9.1"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "node-svn-ultimate": "^1.2.1",
    "yeoman-generator": "^3.2.0",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "prepublish": "tsc",
    "build": "tsc",
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "git@github.com:daemonweb/daemon.git",
  "license": "MIT"
}
